{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [
      {
        "source": "/admin.html",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-cache, no-store, must-revalidate"
          }
        ]
      }
    ],
    "rewrites": [
      {
        "source": "/item_detalhe.html",
        "function": "app"
      },
      {
        "source": "/noticia.html",
        "function": "app"
      }
    ]
  },
  "functions": {
    "source": "functions"
  }
}
